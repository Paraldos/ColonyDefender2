_global_script_class_icons={
"PowerUp": ""
}

[application]

config/name="Colony Defender 2"
run/main_scene="res://menu/StartMenu.tscn"
config/icon="res://img/EnergyContainer.png"

[autoload]

Utils="*res://Tec/Utils.tscn"
SceneTransition="*res://Tec/SceneTransition.tscn"
MyDb="*res://Tec/myDB.gd"
